<div class="container d-flex flex-column justify-content-center h-100">
  <h1>Cartes du jour</h1>
  <div class="cards align-items-center">
      <% @cards.each do |card| %>
        <div class="card">
          <%= cl_image_tag card.parent.photo.key, alt: "card", class: "card-inner" %>
        </div>
      <% end %>
  </div>
</div>
